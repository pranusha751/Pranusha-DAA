# Undirected weights without using functions and in a simple way

# Define the number of vertices
num_vertices = 5

# Create an adjacency matrix initialized to 0
adjacency_matrix = [[0 for _ in range(num_vertices)] for _ in range(num_vertices)]

# Add edges with weights
adjacency_matrix[0][1] = 2
adjacency_matrix[1][0] = 2  # Undirected edge

adjacency_matrix[0][2] = 3
adjacency_matrix[2][0] = 3  # Undirected edge

adjacency_matrix[1][2] = 4
adjacency_matrix[2][1] = 4  # Undirected edge

adjacency_matrix[1][3] = 5
adjacency_matrix[3][1] = 5  # Undirected edge

adjacency_matrix[2][4] = 6
adjacency_matrix[4][2] = 6  # Undirected edge

# Print the adjacency matrix
for row in adjacency_matrix:
    print(row)
